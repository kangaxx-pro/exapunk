; LAST STOP SNAXNET

LINK 800
LINK 799
MODE
MARK DISPATCH
COPY M X
TEST X = 0
TJMP FINITO
TEST #ZGC0 = X
TJMP DISP_0
TEST #ZGC1 = X
TJMP DISP_1
TEST #ZGC2 = X
TJMP DISP_2
TEST #ZGC3 = X
TJMP DISP_3
TEST #ZGC4 = X
TJMP DISP_4

MARK DISP_0
COPY 0  X
JUMP SENDG

MARK DISP_1
COPY 1 X
JUMP SENDG

MARK DISP_2
COPY 2 X
JUMP SENDG

MARK DISP_3
COPY 3 X
JUMP SENDG

MARK DISP_4
COPY 4 X
JUMP SENDG

MARK SENDG
MODE
COPY X M
COPY M T
COPY X M
MODE
COPY 1 M
JUMP DISPATCH


MARK FINITO
MODE
COPY -1 M
HALT